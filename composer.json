{
    "name": "wimski/laravel-model-repositories",
    "description": "Laravel model repositories",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "wimski",
        "laravel",
        "model",
        "repository"
    ],
    "authors": [
        {
            "name": "Wim Reckman",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/wimski/laravel-model-repositories",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0",
        "nesbot/carbon": "^2.62.1 || ^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "nunomaduro/larastan": "^1.0 || ^2.0",
        "orchestra/testbench": "^6.23 || ^7.0 || ^8.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Wimski\\ModelRepositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wimski\\ModelRepositories\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "analyze": "@php vendor/phpstan/phpstan/phpstan analyse --memory-limit 1G",
        "coverage": "@php vendor/phpunit/phpunit/phpunit",
        "test": "@php vendor/phpunit/phpunit/phpunit --no-coverage"
    }
}
